<Window
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d"
	x:Class="Timo.MainWindow"
	x:Name="Window"
	Title="Timo 1.0"
	Width="1113.91" Height="695.94" Background="{x:Null}" AllowsTransparency="True" WindowStyle="None" WindowStartupLocation="CenterScreen">

    <Grid x:Name="LayoutRoot">
        <Rectangle x:Name="Background" Fill="White" PreviewMouseDown="Background_PreviewMouseDown"/>
        <Rectangle x:Name="Logo" Fill="{DynamicResource Timo-logo}" HorizontalAlignment="Left" Height="174" Margin="486,265,0,0" VerticalAlignment="Top" Width="232"/>
        <Rectangle x:Name="MenuPanel" Fill="#FF3B9CCC" HorizontalAlignment="Left" Height="696" VerticalAlignment="Top" Width="163"/>
        <Rectangle Fill="White" HorizontalAlignment="Left" Height="55" Margin="-3,31,0,0" VerticalAlignment="Top" Width="172"/>
        <TextBlock HorizontalAlignment="Left" Margin="15,42,0,0" TextWrapping="Wrap" Text="Home" VerticalAlignment="Top" FontFamily="Corbel" FontSize="25" Foreground="#FF3B9CCC"/>
        <TextBlock HorizontalAlignment="Left" Margin="10,202,0,0" TextWrapping="Wrap" Text="About" VerticalAlignment="Top" FontFamily="Corbel" FontSize="25" Foreground="White"/>
        <TextBlock HorizontalAlignment="Left" Margin="10,128,0,0" TextWrapping="Wrap" Text="Feedback" VerticalAlignment="Top" FontFamily="Corbel" FontSize="25" Foreground="White"/>
    </Grid>
</Window>